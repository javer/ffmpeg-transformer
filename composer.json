{
    "name": "javer/ffmpeg-transformer",
    "type": "library",
    "description": "FFmpeg transformation command builder",
    "keywords": ["ffmpeg", "transformer", "adapter", "command", "builder"],
    "homepage": "https://github.com/javer/ffmpeg-transformer",
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Borodavko",
            "email": "vadim.borodavko@gmail.com",
            "homepage": "https://github.com/javer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "php-ffmpeg/php-ffmpeg": ">=0.7"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.84",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "3.5.*",
        "swivl/php-coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Javer\\FfmpegTransformer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javer\\FfmpegTransformer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
